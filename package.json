    },
    "homepage": "https://github.com/jfaehrmann/portfolio#readme",
    "devDependencies": {
        "@11ty/eleventy": "^1.0.0-beta.8",
        "autoprefixer": "^10.4.0",
        "cssnano": "^5.0.14",
        "luxon": "^2.2.0",
        "parcel": "^2.0.1",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "speedlify-score": "^2.0.2"
    }
