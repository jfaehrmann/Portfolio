{
    "name": "portfolio",
    "version": "0.0.1",
    "description": "",
    "targets": {
        "default": {
            "engines": {
                "browsers": "> 0.5%, last 2 versions, not dead"
            },
            "distDir": "dist/assets/js",
            "publicUrl": ".",
            "sourceMap": true
        }
    },
    "scripts": {
        "start": "npm-run-all --parallel watch:*",
        "watch:eleventy": "eleventy --serve --watch",
        "watch:js": "parcel watch src/assets/js/shell.js",
        "build": "npm-run-all build:*",
        "build:eleventy": "npx eleventy",
        "build:js": "parcel build src/assets/js/shell.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jfaehrmann/portfolio.git"
    },
